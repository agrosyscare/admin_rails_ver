<%= simple_form_for @floor do |f| %>
<div class="row">
  <div class="col-md-6">
    <div class="form-group">
      <%= f.input :name, input_html: { class: "form-control" } %>
    </div>
  </div>
  <div class="col-md-6">
    <div class="form-group">
      <%= f.input :plant_type, input_html: { class: "form-control" } %>
    </div>
  </div>
</div>
<div class="row">
  <div class="col-md-12">
    <div class="form-group">
      <%= f.association :greenhouse, include_blank: false, input_html: { class: "selectpicker form-control" } %>
    </div>
  </div>
</div>
<%= f.button :submit, class:"btn btn-primary pull-right" %>
<% end %>