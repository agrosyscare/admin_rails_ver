<div class="card">
  <div class="card-header card-header-success">
    <h4 class="card-title ">
      <%= PaperTrail::Version.human_title(:index) %>
    </h4>
  </div>
  <div class="card-body">
    <div class="table-responsive">
      <table class="table table-hover" id="deleted-items-dttb" data-source="<%= datatable_admin_deleted_items_path(format: :json) %>">
        <thead>
          <tr>
            <th data-data='id'><%= PaperTrail::Version.human_attribute_name(:id) %></th>
            <th data-data='item_type'><%= PaperTrail::Version.human_attribute_name(:item_type) %></th>
            <th data-data='whodunnit'><%= PaperTrail::Version.human_attribute_name(:whodunnit) %></th>
            <th data-data='created_at'><%= PaperTrail::Version.human_attribute_name(:created_at) %></th>
            <th data-data='buttons' data-orderable="false" data-class-name="all" data-searchable="false" class="skip-export"></th>
          </tr>
        </thead>
        <tbody>
        </tbody>
      </table>
    </div>
  </div>
</div>